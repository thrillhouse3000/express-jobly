{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "anyOf": [
        {"required": ["title"]},
        {"required": ["salary"]},
        {"required": ["equity"]}
    ],
    "properties": {
        "title": {
            "type": "string",
            "default": "",
            "title": "The title Schema",
            "examples": [
                "Engineer"
            ]
        },
        "salary": {
            "type": "integer",
            "default": 0,
            "title": "The salary Schema",
            "examples": [
                90000
            ]
        },
        "equity": {
            "type": "number",
            "default": 0.0,
            "title": "The equity Schema",
            "examples": [
                11.112
            ]
        }
    },
    "examples": [{
        "title": "Engineer",
        "salary": 90000,
        "equity": 11.112
    }]
}